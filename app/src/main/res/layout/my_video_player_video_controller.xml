<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutDirection="ltr"
    android:orientation="vertical"
    tools:targetApi="30">

    <com.paulocoutinho.rvplayer.ui.exoplayer.DefaultTimeBarNotChange
        android:id="@id/exo_progress"
        android:layout_width="0dp"
        android:layout_height="4dp"
        android:layout_marginStart="0dp"
        android:outlineAmbientShadowColor="@color/transparent"
        android:outlineSpotShadowColor="@color/transparent"
        android:paddingStart="-10dp"
        android:paddingEnd="-10dp"
        app:ad_marker_color="@color/transparent"
        app:bar_height="4dp"
        app:buffered_color="@color/blue_400"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:played_ad_marker_color="@color/transparent"
        app:played_color="@color/red_800"
        app:scrubber_color="@color/transparent"
        app:unplayed_color="@color/blue_200" />

</androidx.constraintlayout.widget.ConstraintLayout>
